{
    "name": "arcgis-js-api-test-react",
    "author": "Brian Wilson bwilson@co.clatsop.or.us",
    "version": "1.0.0",
    "description": "ArcGIS JS 4.x API test, React example",
    "private": false,
    "homepage": "https://delta.co.clatsop.or.us/apps/arcgis-js-api/",
    "scripts": {
        "copy": "cp -R ./node_modules/@arcgis/core/assets .",
        "start": "parcel serve index.html --open",
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "parcel build index.html -d build -o default.htm  --public-url ./ --experimental-scope-hoisting --detailed-report",
        "clean": "rm -rf node_modules/ dist/"
    },
    "keywords": [],
    "license": "MIT",
    "dependencies": {
        "@arcgis/core": "^4.23",
        "@fortawesome/fontawesome-svg-core": "^1.2.20",
        "@fortawesome/free-solid-svg-icons": "^5.10.1",
        "@fortawesome/react-fontawesome": "^0.1.4",
        "bootstrap": "^5.1.3",
        "dgrid": "^1.3.3",
        "esri": "0.0.1-security",
        "esri-loader": "^2.16.0",
        "prop-types": "^15.8.1",
        "query-string": "^7.1.1",
        "react": "^18.0.0",
        "react-bootstrap": "^2.3.0",
        "react-dom": "^18.0.0",
        "react-redux": "^8.0.1",
        "redux": "^4.2.0",
        "redux-first-router": "^2.1.5",
        "redux-first-router-link": "^2.1.1",
        "redux-logger": "^3.0.6"
    },
    "devDependencies": {
        "@babel/core": "^7.17.9",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-react": "^7.16.7",
        "parcel": "^2.5.0"
    },
    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
        ],
        "plugins": [
            "@babel/plugin-proposal-object-rest-spread",
            [
                "@babel/plugin-proposal-class-properties",
                {
                    "loose": true
                }
            ],
            [
                "@babel/plugin-proposal-private-methods",
                {
                    "loose": true
                }
            ]
        ]
    }
}
